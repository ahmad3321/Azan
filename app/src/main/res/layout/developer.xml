<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/loginParent"
    android:background="@drawable/back_developer"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textt"
        style="@style/headerText"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/ges"
        android:inputType="textMultiLine"
        android:lineHeight="50dp"
        android:text="@string/develop"
        android:textColor="@color/col12"
        android:textSize="28sp" />

    <TextView
            android:id="@+id/text1"
            style="@style/headerText"
            android:layout_below="@+id/textt"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/jaz"
            android:text="م.ياسر الهندي"
            android:textColor="@color/black"
            android:textSize="25sp" />
        <TextView
            android:id="@+id/text2"
            style="@style/headerText"
            android:layout_below="@+id/text1"
            android:layout_gravity="center"
            android:layout_marginTop="30dp"
            android:fontFamily="@font/jaz"
            android:text="م.حافظ آغا"
            android:textColor="@color/black"
            android:textSize="25sp" />
    <TextView
        android:id="@+id/text22"
        style="@style/headerText"
        android:layout_below="@+id/text2"
        android:layout_gravity="center"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/jaz"
        android:text="@string/Communicate"
        android:textColor="@color/black"
        android:textSize="25sp" />
    <TextView
        android:id="@+id/text_comunec"
        style="@style/headerText"
        android:layout_below="@+id/text22"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/jaz"
        android:inputType="phone"
        android:lineHeight="50dp"
        android:text="+352681562193"
        android:textColor="@color/col12"
        android:textSize="28sp" />
    </RelativeLayout>
